package ejemploGlobal;

public class Usuario {
	private int id;
	private StringBuffer usuario;
	
	public Usuario(int id, StringBuffer usuario) {
		super();
		this.id = id;
		this.usuario = usuario;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public StringBuffer getUsuario() {
		return usuario;
	}

	public void setUsuario(StringBuffer usuario) {
		this.usuario = usuario;
	}

	@Override
	public String toString() {
		return "Usuario [id=" + id + ", usuario=" + usuario + "]";
	}
	

}
