package ejemploGlobal;

public class Contrasenia {
	
	private int id;
	private StringBuffer contra;
	
	public Contrasenia(int id, StringBuffer contra) {
		super();
		this.id = id;
		this.contra = contra;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public StringBuffer getContra() {
		return contra;
	}

	public void setContra(StringBuffer contra) {
		this.contra = contra;
	}

	@Override
	public String toString() {
		return "Contrasenia [id=" + id + ", contra=" + contra + "]";
	}
	
}
